{
  "format_version": "1.10.0",
  "animation_controllers": {
    "controller.animation.pokemon.gardevoir": {
      "initial_state": "default",
      "states": {
        "default": {
          "transitions": [
            {
              "ground_idle": "v.is_ground_idling"
            },
            {
              "air_idle": "v.is_air_idling"
            },
            {
              "water_idle": "v.is_water_idling"
            },
            {
              "walking": "v.is_walking"
            },
            {
              "swimming": "v.is_swimming"
            },
            {
              "flying": "v.is_flying"
            },
            {
              "sleeping": "v.is_sleeping"
            }
          ],
          "blend_transition": 0.3
        },
        "ground_idle": {
          "animations": [
            {
              "default_ground_idle": "v.skin_index == 0"
            },
            {
              "christmas_ground_idle": "v.skin_index == 1"
            }
          ],
          "transitions": [
            {
              "default": "!v.is_ground_idling"
            }
          ],
          "blend_transition": 0.3
        },
        "air_idle": {
          "animations": [
            {
              "default_air_idle": "v.skin_index == 0 || v.skin_index == 1"
            }
          ],
          "transitions": [
            {
              "default": "!v.is_air_idling"
            }
          ],
          "blend_transition": 0.3
        },
        "water_idle": {
          "animations": [
            {
              "default_water_idle": "v.skin_index == 0"
            },
            {
              "christmas_water_idle": "v.skin_index == 1"
            }
          ],
          "transitions": [
            {
              "default": "!v.is_water_idling"
            }
          ],
          "blend_transition": 0.3
        },
        "walking": {
          "animations": [
            {
              "default_walking": "v.skin_index == 0"
            },
            {
              "christmas_walking": "v.skin_index == 1"
            }
          ],
          "transitions": [
            {
              "default": "!v.is_walking"
            }
          ],
          "blend_transition": 0.3
        },
        "flying": {
          "animations": [
            {
              "default_flying": "v.skin_index == 0 || v.skin_index == 1"
            }
          ],
          "transitions": [
            {
              "default": "!v.is_flying"
            }
          ],
          "blend_transition": 0.3
        },
        "swimming": {
          "animations": [
            {
              "default_swimming": "v.skin_index == 0"
            },
            {
              "christmas_swimming": "v.skin_index == 1"
            }
          ],
          "transitions": [
            {
              "default": "!v.is_swimming"
            }
          ],
          "blend_transition": 0.3
        },
        "sleeping": {
          "animations": [
            {
              "default_sleeping": "v.skin_index == 0"
            },
            {
              "christmas_sleeping": "v.skin_index == 1"
            }
          ],
          "transitions": [
            {
              "default": "!v.is_sleeping"
            }
          ],
          "blend_transition": 0.3
        }
      }
    },
    "controller.animation.pokemon.gardevoir_blink": {
      "initial_state": "default",
      "states": {
        "default": {
          "animations": [
            {
              "default_blink": "v.skin_index == 0"
            }
          ]
        }
      }
    },
    "controller.animation.pokemon.gardevoir_attack": {
      "initial_state": "attack",
      "states": {
        "attack": {
          "animations": [
            {
              "default_attack": "v.skin_index == 0"
            },
            {
              "christmas_attack": "v.skin_index == 1"
            }
          ]
        }
      }
    }
  }
}
