{
  "format_version": "1.10.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "pokeb:item_holder",
      "materials": {
        "default": "zombie"
      },
      "textures": {
        "default": "textures/entity/empty"
      },
      "geometry": {
        "default": "geometry.item_holder"
      },
      "scripts": {
        "scale": "q.property('pokeb:scale')",
        "animate": [
          "idle"
        ]
      },
      "animations": {
        "shake": "animation.item_holder.shake",
        "idle": "animation.item_holder.idle"
      },
      "render_controllers": [
        "controller.render.default"
      ],
      "enable_attachables": true
    }
  }
}