{
  "format_version": "1.18.3",
  "minecraft:client_entity": {
    "description": {
      "identifier": "pokemon:rapidash",
      "materials": {
        "default": "custom_animated",
        "aura": "charged_creeper"
      },
      "textures": {
        "evo_aura": "textures/entity/evolving",
        "default": "textures/entity/pokemon/rapidash/rapidash.png",
        "shiny_default": "textures/entity/pokemon/rapidash/shiny_rapidash.png"
      },
      "scripts": {
        "animate": [
          "controller",
          {
            "blink": "math.random(0.0, 25.0) <= 0.25"
          }
        ]
      },
      "animations": {
        "blink": "animation.rapidash.blink",
        "ground_idle": "animation.rapidash.ground_idle",
        "air_idle": "animation.rapidash.ground_idle",
        "water_idle": "animation.rapidash.water_idle",
        "walking": "animation.rapidash.walking",
        "flying": "animation.rapidash.ground_idle",
        "swimming": "animation.rapidash.swimming",
        "sleeping": "animation.rapidash.sleeping",
        "attack": "animation.rapidash.attack",
        "look_at_target": "animation.pokemon.look_at_target",
        "controller": "controller.animation.pokemon"
      },
      "render_controllers": [
        {
          "controller.render.pokemon:rapidash": "query.variant==0"
        },
        {
          "controller.render.evolve": "query.variant==1"
        }
      ],
      "spawn_egg": {
        "texture": "rapidash",
        "texture_index": 0
      },
      "geometry": {
        "default": "geometry.rapidash"
      }
    }
